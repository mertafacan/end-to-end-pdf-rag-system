# OpenRouter API key (https://openrouter.ai/)
OPENROUTER_API_KEY=your-openrouter-api-key

# Google API key for embedding
GOOGLE_API_KEY=your-google-api-key

# Qdrant vector database URL
QDRANT_URL=http://localhost:6333

# Qdrant collection name
QDRANT_COLLECTION=rag_collection

# Tenant ID (for multi-user support)
TENANT_ID=default

# Rerank model name (e.g. HuggingFace model name)
RERANK_MODEL_NAME=cross-encoder/ms-marco-MiniLM-L6-v2

# Number of results to fetch for rerank
RERANK_TOPK=6

# Number of results to select after rerank
RERANK_CHOOSE=4

# Raw threshold value for rerank
RERANK_THRESHOLD_RAW=0.35

# LLM (Large Language Model) name (e.g. OpenRouter model)
# Note: Since litellm is used, you can also use different models such as OpenAI, Anthropic, Google, Mistral, Together.
LLM_MODEL=openrouter/moonshotai/kimi-k2:free

# Is rerank enabled? (True/False)
RERANK_ENABLED=True

# API server address
API_URL=http://localhost:8000

# ...add new keys if needed...